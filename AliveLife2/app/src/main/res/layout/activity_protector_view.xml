<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#131231"
    android:textAlignment="center"><![CDATA[
    tools:ignore="MissingConstraints" />


    ]]>

    <ImageView
        android:id="@+id/redButton"
        android:layout_width="216dp"
        android:layout_height="216dp"
        android:backgroundTint="#00000E25"
        android:src="@drawable/avatar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/whiteSquare"
        android:layout_width="334dp"
        android:layout_height="126dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.735"
        app:srcCompat="@drawable/square_white"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/whiteSquare3"
        android:layout_width="334dp"
        android:layout_height="126dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.907"
        app:srcCompat="@drawable/square_white"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/whiteSquare2"
        android:layout_width="334dp"
        android:layout_height="126dp"
        android:layout_marginStart="2dp"
        android:backgroundTint="#BCBAC5"
        app:layout_constraintBottom_toBottomOf="@+id/whiteSquare3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.621"
        app:srcCompat="@drawable/square_white"
        tools:ignore="MissingConstraints" />


    <ImageButton
        android:id="@+id/userViewBttn"
        android:layout_width="238dp"
        android:layout_height="83dp"
        android:layout_marginBottom="644dp"
        android:backgroundTint="#00F1F0F4"
        android:gravity="top"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/user_view_btn" />

    <TextView
        android:id="@+id/UserViewTest"
        android:layout_width="148dp"
        android:layout_height="45dp"
        android:text="Protector"
        android:textColor="#C2FFFFFF"
        android:textSize="90px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/locationPicture"
        android:layout_width="219dp"
        android:layout_height="47dp"
        app:layout_constraintBottom_toBottomOf="@+id/whiteSquare2"
        app:layout_constraintEnd_toEndOf="@+id/whiteSquare2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/whiteSquare2"
        app:layout_constraintTop_toTopOf="@+id/whiteSquare2"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/location_user_view"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/photoPicture"
        android:layout_width="219dp"
        android:layout_height="47dp"
        app:layout_constraintBottom_toBottomOf="@+id/whiteSquare"
        app:layout_constraintEnd_toEndOf="@+id/whiteSquare"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/whiteSquare"
        app:layout_constraintTop_toTopOf="@+id/whiteSquare"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/photo_user_view"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/audioPicture"
        android:layout_width="219dp"
        android:layout_height="47dp"
        app:layout_constraintBottom_toBottomOf="@+id/whiteSquare3"
        app:layout_constraintEnd_toEndOf="@+id/whiteSquare3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/whiteSquare3"
        app:layout_constraintTop_toTopOf="@+id/whiteSquare3"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/audio_user_view"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/settingsBottom"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        app:srcCompat="@drawable/conf_down_btn"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <Spinner
        android:id="@+id/protecteesList"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:drawable/btn_dropdown"
        android:backgroundTint="@color/dark_blue"
        android:spinnerMode="dropdown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/protecteesText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/protectees"
        android:gravity="center"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/protecteesList"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/protecteesList" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="63dp" />


</androidx.constraintlayout.widget.ConstraintLayout>

